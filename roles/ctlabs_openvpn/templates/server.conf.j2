#{# /etc/openvpn/server/server.conf #}

dev          tun
persist-tun 

proto        udp
ifconfig     {{ ansible_local.ctlabs_openvpn.server_ip }} {{ ansible_local.ctlabs_openvpn.client_ip }}

secret       ovpn.key
auth         SHA512
auth-nocache

cipher       AES-256-CBC
keepalive    10 60 
verb         3